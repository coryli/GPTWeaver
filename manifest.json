{
    "manifest_version": 3,
    "name": "HTML Renderer",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
        {
          "matches": ["https://chat.openai.com/*"],
          "js": ["content.js"]
        }
      ],
      "background": {
        "service_worker": "background.js"
      }
  }