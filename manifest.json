{
    "manifest_version": 3,
    "name": "GPTWeaver",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://chat.openai.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["sandbox.html"],
            "matches": ["<all_urls>"]
        }
    ]
}